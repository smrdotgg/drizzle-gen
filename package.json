{
  "name": "drizzle-gen",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smrdotgg/drizzle-gen.git"
  },
  "bin": {
    "drizzle-gen": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/index.js",
     "release:alpha": "git add . && git commit -m \"alpha: $npm_config_msg\" && npm version prerelease --preid=alpha && npm publish --tag alpha && git push",
    "prepublish": "npm run build",
    "pushoutthere": "npm version patch && npm run build && npm publish"
  },
  "version": "0.0.15-alpha.1",
  "devDependencies": {
    "@types/node": "^22.10.10",
    "drizzle-kit": "^0.30.2",
    "drizzle-orm": "^0.38.4",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "prettier": "^3.4.2"
  }
}
